    "@parcel/transformer-pug": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@prettier/plugin-pug": "^2.1.1",
    "bootstrap": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.6.4",
    "stylelint": "^14.9.1",
    "stylelint-scss": "^4.3.0"
  }
